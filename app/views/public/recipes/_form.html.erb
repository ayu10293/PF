<div class="form-group">
  <%= f.label :title, "タイトル" %>
  <%= f.text_field :title, class: "form-control" %>
</div>
<div class="form-group">
  <%= f.label :body, "内容" %>
  <%= f.text_area :body, rows: 5, class: "form-control" %>
</div>
<div class="form-group">
  <%= f.file_field :images, multiple: true, accept: "image/*" %>
</div>
<div class="form-group">
  <%= f.submit 'レシピを投稿', class: "btn btn-primary" %>
</div>